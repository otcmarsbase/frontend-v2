{
  "name": "@otcmarsbase/frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@berish/class": "^1.0.2",
    "@berish/serber": "^1.5.0",
    "@chakra-ui/react": "^2.7.1",
    "@craco/craco": "^7.1.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/inter": "^5.0.5",
    "@hookform/resolvers": "^3.1.1",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@web3modal/ethereum": "^2.7.0",
    "browserify-zlib": "^0.2.0",
    "crypto-browserify": "^3.12.0",
    "date-fns": "^2.30.0",
    "decimal.js": "^10.4.3",
    "focus-visible": "^5.2.0",
    "framer-motion": "^10.12.21",
    "https-browserify": "^1.0.0",
    "lodash": "^4.17.21",
    "mobx": "^6.9.0",
    "mobx-persist-store": "^1.1.3",
    "mobx-react-lite": "^3.4.3",
    "react": "^18.2.0",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-scripts": "5.0.1",
    "react-time-picker": "^6.2.1",
    "router5": "^8.0.1",
    "router5-plugin-browser": "^8.0.1",
    "router5-plugin-logger": "^8.0.1",
    "sass": "^1.63.6",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "typescript": "^4.4.2",
    "uuid": "^9.0.0",
    "viem": "^1.3.1",
    "wagmi": "^1.3.9",
    "web-vitals": "^2.1.0",
    "yup": "^1.2.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "clean": "rm -rf node_modules && rm -f yarn.lock && rm -f tsconfig.tsbuildinfo",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "gen:theme-types": "chakra-cli tokens src/shared/theme/index.ts --out src/theme.d.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      },
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@chakra-ui/cli": "^2.4.1",
    "@types/lodash": "^4.14.195",
    "@types/uuid": "^9.0.2",
    "lint-staged": "^13.2.3"
  }
}
