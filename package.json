{
  "name": "@otcmarsbase/frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "rm -f tsconfig.tsbuildinfo && rm -rf dist",
    "clean:full": "rm -rf node_modules && rm -f yarn.lock && npm run clean",
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "craco eject",
    "gen:theme-typings": "chakra-cli tokens src/app/theme/index.ts --out theme-typings.d.ts",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "// ----- Checks -----": "",
    "lint": "eslint \"src/**/*.+(ts|js|tsx|jsx|css)\"",
    "lint:fix": "yarn lint --fix",
    "prettier": "prettier --write \"src/**/*.+(ts|js|tsx|jsx|css)\"",
    "format": "yarn prettier && yarn lint:fix",
    "typecheck": "tsc --noEmit",
    "check:full": "npm run lint:code && npm run typecheck",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.+(ts|js|tsx|jsx|json|css|scss|md)": [
      "npm run format"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@berish/class": "^1.0.2",
    "@berish/linq": "^1.2.0",
    "@berish/serber": "^1.5.0",
    "@chakra-ui/icons": "^2.1.0",
    "@chakra-ui/react": "^2.7.1",
    "@craco/craco": "^7.1.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@fontsource-variable/inter": "^5.0.5",
    "@hookform/resolvers": "^3.1.1",
    "@tanstack/react-table": "^8.9.3",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^20.4.8",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@wagmi/core": "^1.3.9",
    "axios": "^1.4.0",
    "browserify-zlib": "^0.2.0",
    "chakra-react-select": "^4.7.0",
    "crypto-browserify": "^3.12.0",
    "csstype": "^3.1.2",
    "date-fns": "^2.30.0",
    "decimal.js": "^10.4.3",
    "ethers": "^6.7.0",
    "focus-visible": "^5.2.0",
    "framer-motion": "^10.12.21",
    "https-browserify": "^1.0.0",
    "husky": "^8.0.3",
    "i18next": "^23.4.4",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mobx": "^6.9.0",
    "mobx-persist-store": "^1.1.3",
    "mobx-react-lite": "^3.4.3",
    "numerable": "^0.3.15",
    "qs": "^6.11.2",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.16.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.45.1",
    "react-i18next": "^13.1.2",
    "react-icons": "^4.10.1",
    "react-scripts": "5.0.1",
    "react-time-picker": "^6.2.1",
    "react-use": "^17.4.0",
    "router5": "^8.0.1",
    "router5-plugin-browser": "^8.0.1",
    "router5-plugin-logger": "^8.0.1",
    "sass": "^1.63.6",
    "stream-browserify": "^3.0.0",
    "stream-http": "^3.2.0",
    "ts-jest": "^29.1.1",
    "typescript": "^4.4.2",
    "uuid": "^9.0.0",
    "viem": "^1.9.0",
    "wagmi": "^1.3.10",
    "web-vitals": "^2.1.0",
    "yup": "^1.2.0"
